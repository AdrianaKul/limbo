#! /usr/bin/env python
import limbo

def build(bld):
    limbo.create_variants(bld,
                           source = 'multi.cpp',
                           uselib_local = 'limbo',
                           uselib = 'BOOST EIGEN TBB SFERES',
                           variants = ['PAREGO ZDT1 DIM30',
                                       'PAREGO ZDT2 DIM30',
                                       'PAREGO ZDT3 DIM30',
                                       'PAREGO ZDT1 DIM30',
                                       'PAREGO ZDT2 DIM6',
                                       'PAREGO ZDT3 DIM6',
                                       'NS_EGO ZDT1 DIM30',
                                       'NS_EGO ZDT2 DIM30',
                                       'NS_EGO ZDT2 DIM2',
                                       'NS_EGO ZDT3 DIM30',
                                       'NS_EGO ZDT1 DIM6',
                                       'NS_EGO ZDT2 DIM6',
                                       'NS_EGO ZDT3 DIM6'
                                       ],)
