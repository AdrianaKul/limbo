#! /usr/bin/env python
import limbo

def build(bld):
    limbo.create_variants(bld,
                           source = 'multi.cpp',
                           uselib_local = 'limbo',
                           uselib = 'BOOST EIGEN TBB SFERES',
                           variants = ['PAREGO ZDT2',
                                       'PAREGO MOP2',
                                       'NS_EGO ZDT2',
                                       'NS_EGO MOP2'
                                       ],)
